import java.util.Scanner;
public class HelloWorld{
     static int n,n1=0,r;
     public static void main(String []args){
        Scanner s=new Scanner(System.in);
        System.out.println("enter the number");
       n=s.nextInt();
       while(n>0)
       {
           r=n%10;
           n1=n1*10+r;
           n=n/10;
       }
       System.out.println(n1);
     }
}
