import java.util.Scanner;
public class HelloWorld{

     public static void main(String []args){
        int n,b=0;
        Scanner s= new Scanner(System.in);
       System.out.println("enter the number");
       n=s.nextInt();
       while(n>0)
       {
           n=n/10;
           b++;
       }
        
            System.out.println(b);
     
       
}
}
