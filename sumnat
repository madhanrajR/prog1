import java.util.Scanner;
public class sumnat{

     public static void main(String []args){
         int i,n,sum=0;
         Scanner scanner = new Scanner(System.in);
         System.out.println("enter the limit");
         n=scanner.nextInt();
        for(i=1;i<=n;i++)
            sum=sum+i;
        System.out.println(sum);
     }
}
